<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <SolidColorBrush x:Key="HighlightDark" Color="#006769"/>
    <SolidColorBrush x:Key="Background" Color="#1e1e1e" />
    <SolidColorBrush x:Key="Foreground" Color="#2d2d30"/>
    <SolidColorBrush x:Key="HighlightLight" Color="#40A578"/>
    <SolidColorBrush x:Key="Text" Color="#e6e6e6"/>
    <ImageBrush x:Key="WindowBackground" 
            ImageSource="darkTheme.png" 
            Stretch="UniformToFill" />
    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="./FontSize.xaml" />
    </ResourceDictionary.MergedDictionaries >
</ResourceDictionary>